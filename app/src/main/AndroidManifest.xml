<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ngopidev.project.cakeuapps">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
                android:name=".allAct.LoginAct"
                android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".allAct.MainActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".allAct.RegisterAct"
                  android:screenOrientation="portrait"/>
        <activity android:name=".allAct.FirstMoney"
                android:screenOrientation="portrait"/>
        <activity android:name=".allAct.CashFlowAct.CashFlowMain"
                android:screenOrientation="portrait"
                />

        <!-- add ads id-->
        <meta-data
                android:name="com.google.android.gms.ads.APPLICATION_ID"
                android:value="@string/adsId"/>
    </application>

</manifest>